  "step1": {
    "title": "Skin to skin counselling",
    "fields": [
      {
        "key": "skin_to_skin_counselling",
        "openmrs_entity_parent": "",
        "openmrs_entity": "concept",
        "openmrs_entity_id": "skin_to_skin_counselling",
        "type": "native_radio",
        "label": "Do you know why small babies need to stay extra warm in the first days of life? (Give the mother/father or caregiver a chance to respond)",
        "label_text_style": "bold",
        "label_info_title": "Skin to skin counselling",
        "dynamic_label_info": [
          {
            "dynamic_label_title": "",
            "dynamic_label_text": "Small babies have less fat than larger babies, so low body temperature can be a major issue. Make sure your baby is dry and warm all the time.",
            "dynamic_label_image_src": ""
          },
          {
            "dynamic_label_title": "",
            "dynamic_label_text": "Kangaroo mother care (KMC) is an excellent way for mothers/fathers or caregivers to keep these babies warm at all times. The skin-to-skin contact of KMC helps keep the baby's body temperature normal.\n",
            "dynamic_label_image_src": ""
          },
          {
            "dynamic_label_title": "",
            "dynamic_label_text": "Note to CHW: Emphasize that anyone can do KMC (including mother/father or caregiver)",
            "dynamic_label_image_src": ""
          }
        ],
        "label_is_dynamic": true,
        "values": [
          "Yes",
          "No"
        ],
        "options": [
          {
            "key": "Yes",
            "text": "Yes",
            "openmrs_entity": "concept",
            "openmrs_entity_id": "Yes"
          },
          {
            "key": "No",
